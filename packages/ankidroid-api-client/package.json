{
  "name": "ankidroid-api-client",
  "version": "0.1.0",
  "description": "TypeScript client for AnkiDroid API Tauri plugin",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --sourcemap --watch",
    "typecheck": "tsc --noEmit",
    "quickfix": "echo 'No linting configured yet'"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist"
  ]
}